<%= form_for @dog do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <%= f.label :breed %>
    <%= f.text_field :breed %><br>
    <%= f.label :age %>
    <%= f.text_field :age %><br>
    <%= f.label "Employees" %><br>
    <%= f.fields_for :employees, @dog.employees.build do |emp_fields| %>
        <%= emp_fields.text_field :name %>
    <% end %><br>
    <%= f.submit %><br>
<% end %>